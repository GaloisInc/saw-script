// This script expects to be run from the saw-script root directory
enable_experimental
env <- heapster_init_env "xor_swap" "examples/heapster/xor_swap.bc"
heapster_typecheck_fun env "xor_swap" "(x:bv 64, y:bv 64). arg0: ptr((W,0) |-> eq(llvmword(x))), arg1: ptr((W,0) |-> eq(llvmword(y))) -o arg0: ptr((W,0) |-> exists z:bv 64.eq(llvmword(z))), arg1: ptr((W,0) |-> exists z:bv 64.eq(llvmword(z))), ret:true"
heapster_export_coq env "examples/heapster-coq/xor_swap.v"
