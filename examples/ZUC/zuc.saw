F_spec cry_F = do {
    X0 <- llvm_var "BRC_X0" (llvm_int '32);
    X1 <- llvm_var "BRC_X1" (llvm_int '32);
    X2 <- llvm_var "BRC_X2" (llvm_int '32);
    R1 <- llvm_var "F_R1" (llvm_int '32);
    R2 <- llvm_var "F_R2" (llvm_int '32);
    S0 <- llvm_var "S0" (llvm_array '256 (llvm_int '8));
    S1 <- llvm_var "S1" (llvm_array '256 (llvm_int '8));
    llvm_assert (S0 == S0Table);
    llvm_assert (S1 == S1Table);
    let res = cry_F ([X0, X1, X2], [R1, R2]) : ([32], [32], [32]);
    llvm_return (res.1);
    llvm_ensure_eq "F_R1" (res.2);
    llvm_ensure_eq "F_R2" (res.3);
    llvm_verify_tactic abc;
};

BR_spec : ([16][31] -> ([32], [32], [32], [32])) -> LLVMSetup ();
BR_spec cry_BR = do {
    (S0 : [32]) <- llvm_var "LFSR_S0" (llvm_int '32);
    (S1 : [32]) <- llvm_var "LFSR_S1" (llvm_int '32);
    (S2 : [32]) <- llvm_var "LFSR_S2" (llvm_int '32);
    (S3 : [32]) <- llvm_var "LFSR_S3" (llvm_int '32);
    (S4 : [32]) <- llvm_var "LFSR_S4" (llvm_int '32);
    (S5 : [32]) <- llvm_var "LFSR_S5" (llvm_int '32);
    (S6 : [32]) <- llvm_var "LFSR_S6" (llvm_int '32);
    (S7 : [32]) <- llvm_var "LFSR_S7" (llvm_int '32);
    (S8 : [32]) <- llvm_var "LFSR_S8" (llvm_int '32);
