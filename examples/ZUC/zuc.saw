main = do {
    (ref_F : ([3][32], [2][32]) -> ([32], [32], [32])) <- read_sbv "F.sbv";
    (ref_BR : [16][31] -> ([32], [32], [32], [32])) <- read_sbv "BitReorganization.sbv";
    (ref_L1 : [32] -> [32]) <- read_sbv "L1.sbv";
    (ref_L2 : [32] -> [32]) <- read_sbv "L2.sbv";
    llvm_verify "zuc.bc" "BitReorganization" [] (BR_spec ref_BR);
    L1_ov <- llvm_verify "zuc.bc" "L1" [] (L12_spec ref_L1);
    L2_ov <- llvm_verify "zuc.bc" "L2" [] (L12_spec ref_L2);
    llvm_verify "zuc.bc" "F" [L1_ov, L2_ov] (F_spec ref_F);
    print "Done.";
};
