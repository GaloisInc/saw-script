// Test: offline_coq command.
enable_experimental;

prove (offline_coq "test_offline_coq1") {{ \(x:[8]) (y:[8]) -> x == y ==> x+y == x+x }};

prove (offline_coq "test_offline_coq2") {{ \(a:Bit) (b:Bit) (c:Bit) -> (a && b) || (a && c) == a && (b || c) }};

prove (offline_coq "test_offline_coq3") {{ \(x:[16]) (y:[16]) (z:[16]) -> (x + y) + z == x + (y + z) }};

prove (offline_coq "test_offline_coq4") {{ \(b:Bit) (x:[8]) (y:[8]) -> (if b then x else y) == (if ~b then y else x) }};

prove (offline_coq "test_offline_coq5") {{ \(x:[8]) (y:[8]) -> [x, y] == reverse [y, x] }};

prove (offline_coq "test_offline_coq6") {{ \(a:Bit) (b:Bit) (c:Bit) -> (a ==> b) && (b ==> c) ==> (a ==> c) }};
