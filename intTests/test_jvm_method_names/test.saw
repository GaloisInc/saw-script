    this <- jvm_alloc_object "Test";
    x <- jvm_fresh_var "x" java_int;
    jvm_execute_func [this, jvm_term x];
    jvm_field_is this "Test.val" (jvm_term {{ sext x : [64] }});
  }
  z3;

