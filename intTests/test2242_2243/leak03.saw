// Example from #2243 using the Cryptol scope.

enable_experimental;

let foo () = do {
   prove_print do {
      proof_subshell ();
      let _ = run (fails (do { let _ = run (print {{ quack }}); return (); }));
      return ();
   } {{ True }};
};

let bar () = do {
   print {{ quack }};
};

print "this should fail";
foo ();
print "this should also fail";
fails (bar ());
