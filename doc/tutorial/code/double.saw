main = do {
    l <- llvm_load_module "double.bc";
    double_imp <- llvm_extract l "double_imp" llvm_pure;
    double_ref <- llvm_extract l "double_ref" llvm_pure;
    let thm = {{ \x -> double_ref x == double_imp x }};

    r <- prove abc thm;
    print r;

    let thm_neg = {{ \x -> ~(thm x) }};
    write_smtlib1 "double.smt" thm_neg;

    print "Done.";
};
