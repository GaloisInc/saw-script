main = do {
    double_imp <- llvm_extract "simple.bc" "double_imp" llvm_pure;
    double_ref <- llvm_extract "simple.bc" "double_ref" llvm_pure;
    print "== Ref:";
    print double_ref;
    print "== Imp:";
    print double_imp;
    eq <- equal double_ref double_imp;
    print "== Eq:";
    print eq;
    r <- prove abc eq;
    print r;
};
