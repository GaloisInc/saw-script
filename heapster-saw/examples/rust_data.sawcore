
module rust_data where

import Prelude;

-- A type description for the list type over a type description T, which should
-- only use free deBruijn indices starting at 1 because it is being substituted
-- inside a Tp_Ind constructor
ListDesc : TpDesc -> TpDesc;
ListDesc T = Tp_Ind (Tp_Ind (Tp_Sum Tp_Unit (Tp_Pair T (Tp_Var 0))));
