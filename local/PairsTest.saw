// 2025-ish version of saw: respecting monadic type at top-level
print "starting";

enable_experimental;
load_sawcore_from_file "Pairs.sawcore";

let printNorm m e =
  do {
   print e;
   print (show_term (normalize_term (parse_core_mod m e)));
   print "";
  };

let printNorm_e2 = printNorm "Pairs";   // Module name!

printNorm_e2 "test1";
printNorm_e2 "test2";
printNorm_e2 "test3";

print "done";
